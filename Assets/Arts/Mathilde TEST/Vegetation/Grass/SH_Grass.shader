				simplePerlin2D211 = simplePerlin2D211*0.5 + 0.5;
				float3 break191 = v.vertex.xyz;
				float temp_output_216_0 = ( ( ( simplePerlin2D211 - 0.2 ) * _WindStrenght ) + break191.x );
				float4 appendResult195 = (float4(temp_output_216_0 , temp_output_216_0 , break191.z , 0.0));
				float2 temp_cast_3 = (1.0).xx;
				float2 temp_cast_4 = (1.0).xx;
				float2 texCoord194 = v.ase_texcoord.xy * temp_cast_3 + temp_cast_4;
				float4 lerpResult193 = lerp( float4( v.vertex.xyz , 0.0 ) , appendResult195 , float4( texCoord194, 0.0 , 0.0 ));
				
				o.ase_texcoord9.xy = v.ase_texcoord.xy;
				
				//setting value to unused interpolator channels and avoid initialization warnings
